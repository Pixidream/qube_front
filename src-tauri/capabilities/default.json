{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://127.0.0.1:3000/*"
        },
        {
          "url": "https://192.168.1.47:3000/*"
        }
      ]
    },
    "deep-link:default",
    "os:default"
  ]
}
